    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron . \""
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-input-slider": "^5.1.2",
    "react-ripples": "^2.2.1",
    "react-scripts": "3.1.1",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "electron": "^6.0.7",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
